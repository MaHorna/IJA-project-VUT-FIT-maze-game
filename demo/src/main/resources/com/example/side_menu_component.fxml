<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>

<VBox fx:id="side_menu_cont" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" style="-fx-background-color: #DDDDDD;" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.menu_controller">
   <children>
      <Button focusTraversable="false" onAction="#animate_hamb_button" fx:id="ham_button" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Show/Close menu">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/hamb.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>      
      <Button focusTraversable="false" onAction="#open_editor_button" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Open editor">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/custom.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button focusTraversable="false" onAction="#save_custom_map_button" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Save custom map">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/save.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button focusTraversable="false" onAction="#load_custom_map_button" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Load custom map">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/play.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button focusTraversable="false" onAction="#play_again_button" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Play last map again">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/replay.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button focusTraversable="false" onAction="#save_replay_button" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Save replay">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/save.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button focusTraversable="false" onAction="#load_replay_button" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Load replay">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/play.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button focusTraversable="false" onAction="#exit_button" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Exit">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/exit.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</VBox>