<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="side_menu_cont" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" style="-fx-background-color: #DDDDDD;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="com.pacman.menu_controller">
   <children>
      <Button fx:id="ham_button" focusTraversable="false" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" onAction="#animate_hamb_button" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Show/Close menu">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/hamb.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button focusTraversable="false" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" onAction="#load_custom_map_button" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Load custom map">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/play.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button focusTraversable="false" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" onAction="#play_again_button" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Play last map again">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/replay.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button focusTraversable="false" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" onAction="#save_replay_button" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Save replay">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/save.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button focusTraversable="false" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" onAction="#load_replay_button" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Load replay">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/play.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button focusTraversable="false" graphicTextGap="20.0" maxWidth="200.0" minWidth="55.0" mnemonicParsing="false" onAction="#exit_button" prefHeight="60.0" prefWidth="200.0" style="-fx-alignment: center-left;" text="Exit">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/exit.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</VBox>
